{
  "name": "vmh_reactjs_cv",
  "version": "0.1.0",
  "homepage": "https://vmhdev.github.io/reactjs_cv",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start": "env-cmd -f .env.dev react-scripts start",
    "build": "env-cmd -f .env.dev react-scripts build",
